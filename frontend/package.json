    "embla-carousel-react": "^8.3.0",
    "framer-motion": "^11.11.7",
    "js-cookie": "^3.0.5",
    "jwt-decode": "^4.0.0",
    "keen-slider": "^6.8.6",
    "lucide-react": "^0.451.0",
    "next": "^14.2.15",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.2.9",
