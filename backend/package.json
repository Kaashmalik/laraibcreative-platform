{
  "name": "laraibcreative-backend",
  "version": "1.0.0",
  "description": "Backend API for LaraibCreative - Custom tailoring e-commerce platform",
  "main": "server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "dev:watch": "nodemon --exec ts-node src/server.ts",
    "seed": "ts-node src/seeds/index.ts",
    "seed:categories": "ts-node src/seeds/categories.seed.ts",
    "seed:products": "ts-node src/seeds/products.seed.ts",
    "seed:settings": "ts-node src/seeds/settings.seed.ts",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.{ts,js}",
    "lint:fix": "eslint src/**/*.{ts,js} --fix",
    "type-check": "tsc --noEmit",
    "backup": "ts-node scripts/backup-db.ts",
    "verify-indexes": "ts-node src/utils/verifyIndexes.ts"
  },
  "keywords": [
    "ecommerce",
    "tailoring",
    "custom-orders",
    "fashion",
    "api"
  ],
  "author": "LaraibCreative",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.8.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.4.0",
    "express-validator": "^7.3.1",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "mongoose": "^8.6.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "nodemailer": "^7.0.9",
    "pdfkit": "^0.15.0",
    "qrcode": "^1.5.4",
    "sanitize-html": "^2.17.0",
    "sharp": "^0.33.5",
    "slugify": "^1.6.6",
    "twilio": "^5.3.2",
    "validator": "^13.12.0",
    "winston": "^3.14.2",
    "winston-daily-rotate-file": "^5.0.0",
    "xss-clean": "^0.1.4",
    "zod": "^3.23.8",
    "rate-limiter-flexible": "^5.0.3",
    "@sentry/node": "^8.0.0",
    "@sentry/profiling-node": "^8.0.0",
    "cls-rtracer": "^2.2.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.0",
    "@types/nodemailer": "^6.4.14",
    "@types/qrcode": "^1.5.5",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.20.0",
    "npm": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/laraibcreative-backend.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/laraibcreative-backend/issues"
  }
}
